SELECT INDIVIDUAl_ID,
       WEEK,
       STREAM_LABEL,
       sum(CAST ( (JulianDay([END]) - JulianDay(START) ) * 24 * 60 * 60 AS INTEGER) ) AS SUM_DURATION_SEC
  FROM audienceData12Weeks
 WHERE (STREAM_LABEL NOT LIKE '%Radio%' AND 
        STREAM_LABEL NOT LIKE '%iPlayer%' AND 
        STREAM_LABEL NOT LIKE '%London 94.9FM%' AND 
        STREAM_LABEL NOT LIKE '%YouTube%') AND 
       (STREAM_LABEL LIKE '%BBC%' OR 
        STREAM_LABEL LIKE '%CBeebies%') AND 
       DATA_TYPE NOT LIKE 'APP' AND 
       INDIVIDUAL_ID NOT LIKE '%DIA%'
 GROUP BY INDIVIDUAL_ID,
          WEEK,
          STREAM_LABEL;
