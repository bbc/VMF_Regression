SELECT INDIVIDUAl_ID,
       WEEK,
       STREAM_LABEL,
       sum(CAST ( (JulianDay([END]) - JulianDay(START) ) * 24 * 60 * 60 AS INTEGER) ) AS SUM_DURATION_SEC
  FROM audienceData12Weeks
 WHERE (STREAM_LABEL LIKE '%BBC%' OR 
        STREAM_LABEL LIKE '%CBEEBIES%') AND 
       (DATA_TYPE LIKE 'LIVE' OR 
        DATA_TYPE LIKE 'VODSAL') AND 
       STREAM_LABEL NOT LIKE '%Radio%' AND 
       INDIVIDUAL_ID NOT LIKE '%DIA%'
 GROUP BY INDIVIDUAL_ID,
          WEEK,
          STREAM_LABEL